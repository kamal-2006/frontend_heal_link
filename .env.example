# Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# Google OAuth Client ID
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id-here

# Backend API URL
# For development: http://localhost:5001/api/v1
# For production: https://your-backend-domain.vercel.app/api/v1
NEXT_PUBLIC_API_BASE_URL=http://localhost:5001/api/v1

# Alternative API URL variable (fallback)
NEXT_PUBLIC_API_URL=http://localhost:5001/api/v1

# Socket.IO URL for real-time notifications
# For development: http://localhost:5001
# For production: https://your-backend-domain.vercel.app
NEXT_PUBLIC_SOCKET_URL=http://localhost:5001
